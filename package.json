{
    "name": "vscode-vega-viewer",
    "displayName": "Vega Viewer",
    "description": "Vega Viewer",
    "version": "0.0.1",
    "publisher": "Random Fractals, Inc.",
    "repository": {
        "url": "https://github.com/RandomFractals/vscode-vega-viewer"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showVegaView"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showVegaView",
                "title": "Vega Viewer: Show Preview"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vega": "^4.4.0",
        "vega-lite": "^3.0.0-rc10"
    }
}
